{
  "name": "tw5-MY-PLUGIN",
  "author": "MY-USER",
  "scripts": {
    "build": "npm run build:html; npm run build:json",
    "build:html": "TIDDLYWIKI_PLUGIN_PATH=plugins tiddlywiki --build index",
    "build:json": "TIDDLYWIKI_PLUGIN_PATH=plugins tiddlywiki --render '$:/plugins/MY-USER/MY-PLUGIN' tw5-MY-PLUGIN.json text/plain '$:/core/templates/json-tiddler'"
  },
  "dependencies": {
    "tiddlywiki": "^5.3.3"
  }
}
